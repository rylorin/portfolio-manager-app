<div class="table-responsive">
  <table class="table table-striped table-sm">
    <thead class="thead-dark">
      <tr class="text-center">
        <th>#</th>
        <th>Symbol</th>
        <th>Currency</th>
        <th>Net Dividends</th>
        <th>Stock P/L</th>
        <th>Options P/L</th>
        <th>Total</th>
      </tr>
    </thead>
    <tbody>
    {% for symbol in stocks %}
        <tr>
          <td class="text-right">{{ symbol.count }}</td>
          <td>
              <a href="{{ path('portfolio_stock_dashboard', {'portfolio': portfolio.id, 'stock': symbol['stockId']}) }}">
                {{ symbol.symbol }}
              </a>
          </td>
          <td>{{ symbol.currency }}</td>
          <td class="{{ symbol.totalNetDividends|my_number_class }}">{{ symbol.totalNetDividends|my_decimal_format }}</td>
          <td class="{{ symbol.totalStockPNL|my_number_class }}">{{ symbol.totalStockPNL|my_decimal_format }}</td>
          <td class="{{ symbol.totalOptionsPNL|my_number_class }}">{{ symbol.totalOptionsPNL|my_decimal_format }}</td>
          <td class="{{ (symbol.totalInterests + symbol.totalStockPNL + symbol.totalOptionsPNL + symbol.totalNetDividends)|my_number_class }}">{{ (symbol.totalStockPNL + symbol.totalOptionsPNL + symbol.totalNetDividends)|my_decimal_format }}</td>
        </tr>
    {% endfor %}
    </tbody>
  </table>
</div>
